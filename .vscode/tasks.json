{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cmake",
			"label": "Eugene build all",
			"command": "build",
			"targets": [
				"all"
			],
			"problemMatcher": {
				"fileLocation": [
					"relative",
					"${workspaceFolder}"
				],
				"pattern": {
					"regexp": "^(ERROR: )(.*):(\\d+):(\\d+):\\s+(.*)$",
					"file": 2,
					"line": 3,
					"column": 4,
					"severity": 1,
					"message": 5
				}
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}